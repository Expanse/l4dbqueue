    "keywords": [
        "laravel",
        "queue",
        "mysql",
		"postgres"
    ],
    "homepage": "https://github.com/Expanse/l4dbqueue",
    "license": "MIT",
    "authors": [
        {
            "name": "Glen Solsberry",
