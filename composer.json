    "require": {
        "php": ">=5.4.0",
        "nesbot/carbon": "~1.0",
        "illuminate/support": "4.2.*"
    },
	"require-dev": {
		"phpunit/phpunit": "4.3.5",
		"mockery/mockery": "~0.9",
        "illuminate/queue": "4.2.*"
